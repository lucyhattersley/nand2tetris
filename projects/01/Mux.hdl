/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Xor(a=a, b=sel, out=q1);
    And(a=b, b=sel, out=q2);
    Or(a=q1, b=q2, out=out);
}
