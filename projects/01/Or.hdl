 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    // Chips: Not(in), Nand(a,b), And(a,b)
    // = ( A NAND A ) NAND ( B NAND B ) 
    Nand(a=a, b=a, out=q);
    Nand(a=q, b=b, out=out);
    
}
